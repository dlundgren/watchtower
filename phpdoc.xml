<?xml version="1.0" encoding="UTF-8" ?>
<phpdoc>
	<title>WatchTower</title>
	<parser>
		<target>/tmp</target>
		<encoding>utf8</encoding>
		<extensions>
			<extension>php</extension>
		</extensions>
	</parser>
	<transformer>
		<target>tmp/phpdoc</target>
	</transformer>
	<transformations>
		<template name="responsive-twig" />
	</transformations>
	<files>
		<directory>src</directory>
	</files>
</phpdoc>